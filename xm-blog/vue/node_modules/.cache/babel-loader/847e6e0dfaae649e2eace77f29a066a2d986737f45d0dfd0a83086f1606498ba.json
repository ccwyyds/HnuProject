{"ast":null,"code":"export default {\n  name: \"Activity\"\n};","map":{"version":3,"names":["name"],"sources":["src/views/front/Activity.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <el-row :gutter=\"10\">\r\n      <el-col :span=\"6\" v-for=\"item in tableData\" :key=\"item.id\">\r\n        <div class=\"card\" style=\"margin-bottom: 10px\">\r\n          <img :src=\"item.cover\" alt=\"\" style=\"width: 100%; height: 150px; border-radius: 5px\">\r\n          <div style=\"margin: 10px 0; font-weight: bold\">{{ item.name }}</div>\r\n          <div style=\"display: flex; align-items: center\">\r\n            <div style=\"flex: 1; color: #666\"><i class=\"el-icon-date\"></i> {{ item.start }}</div>\r\n            <el-button type=\"primary\" disabled v-if=\"item.isEnd\">已结束</el-button>\r\n            <el-button type=\"primary\" v-else @click=\"goDetail(item.id)\">报 名</el-button>\r\n          </div>\r\n        </div>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <div class=\"pagination\" style=\"margin-top: 10px\">\r\n      <el-pagination\r\n          background\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Activity\"\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA8BA;EACAA,IAAA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}